{
  "name": "pelmered/composer-plugin-language-update",
  "description": "Update translations for WP core, themes and plugins where managed by composer.",
  "keywords": [
    "WordPress",
    "Languages",
    "Composer",
    "Plugins",
    "Themes"
  ],
  "homepage": "https://angrycreative.com",
